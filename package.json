{
  "name": "band-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn next:dev\" \"yarn sanity:studio\"",
    "type-check": "tsc -w",
    "next:dev": "NODE_OPTIONS=\"--max_old_space_size=4096\" next dev",
    "next:build": "next build",
    "next:start": "next start",
    "sanity:build": "cd studio && yarn build",
    "sanity:deploy": "cd studio && yarn deploy",
    "sanity:studio": "cd studio && yarn start && cd ..",
    "sanity:login": "sanity login",
    "start": "yarn next:start",
    "build": "yarn next:build",
    "env-pull": "vercel env pull",
    "postinstall": "cd studio && yarn"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@sanity/client": "^2.2.6",
    "@sanity/image-url": "^0.140.22",
    "clsx": "^1.1.1",
    "fast-levenshtein": "^3.0.0",
    "get-youtube-id": "^1.0.1",
    "next": "10.0.9",
    "next-seo": "^4.24.0",
    "react": "17.0.1",
    "react-aspect-ratio": "^1.0.49",
    "react-dom": "17.0.1",
    "react-icons": "^4.2.0",
    "react-photoswipe": "^1.3.0",
    "react-player": "^2.9.0",
    "react-query": "^3.13.0",
    "sharp": "^0.27.2",
    "shopify-buy": "^2.11.0",
    "zustand": "^3.3.3"
  },
  "devDependencies": {
    "@sanity/cli": "^2.6.0",
    "@types/fast-levenshtein": "^0.0.1",
    "@types/node": "^14.14.35",
    "@types/react-photoswipe": "^1.3.0",
    "@types/shopify-buy": "^2.10.5",
    "concurrently": "^6.0.0",
    "typescript": "^4.2.3",
    "vercel": "^23.0.1"
  }
}
