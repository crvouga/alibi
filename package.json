{
  "name": "band-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "sanity:setup": "cd studio && npx sanity install && cd ..",
    "sanity:studio": "cd studio && npx sanity start && cd ..",
    "sanity:build": "cd studio && npx sanity build ../public/studio -y && cd ..",
    "postinstall": "yarn sanity:setup",
    "dev": "concurrently \"yarn next:dev\" \"yarn sanity:studio\"",
    "start": "yarn next:start",
    "build": "yarn sanity:build && yarn next:build"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@sanity/client": "^2.2.6",
    "@sanity/image-url": "^0.140.22",
    "clsx": "^1.1.1",
    "fast-levenshtein": "^3.0.0",
    "get-youtube-id": "^1.0.1",
    "next": "10.0.9",
    "react": "17.0.1",
    "react-aspect-ratio": "^1.0.49",
    "react-dom": "17.0.1",
    "react-icons": "^4.2.0",
    "react-photoswipe": "^1.3.0",
    "react-player": "^2.9.0",
    "react-query": "^3.13.0",
    "sharp": "^0.27.2",
    "zustand": "^3.3.3"
  },
  "devDependencies": {
    "@sanity/cli": "^2.6.0",
    "@types/fast-levenshtein": "^0.0.1",
    "@types/node": "^14.14.35",
    "@types/react-photoswipe": "^1.3.0",
    "concurrently": "^6.0.0",
    "typescript": "^4.2.3"
  }
}
